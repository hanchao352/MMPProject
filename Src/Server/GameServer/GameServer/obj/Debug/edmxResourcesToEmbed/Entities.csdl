    <Property Name="CharactersID" Type="Int32" Nullable="false" />
  </EntityType>
  <Association Name="CharacterItem">
    <End Type="ExtremeWorldModel.TCharacters" Role="TCharacters" Multiplicity="1" />
    <End Type="ExtremeWorldModel.TCharacterItem" Role="TCharacterItem" Multiplicity="*" />
